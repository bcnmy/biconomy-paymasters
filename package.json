    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.30.5",
    "chai": "^4.3.7",
    "merkletreejs": "^0.3.9",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.1",
    "typescript": "^5.2.2",
